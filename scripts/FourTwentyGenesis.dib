    }
    else if (trimmed.StartsWith("- id:") || (trimmed.StartsWith("#") && trimmed.Contains("‚îÄ‚îÄ")))
    {
        inModuleBlock = false;
    }
}

// Don't forget the last module
if (currentId != null && currentName != null)
{
